<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/blankclean"
    tools:context=".MyActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/Xinch"
        android:textColor="#ffffffff"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/Xfeet"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="43dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/Xfeet"
        android:textColor="#ffffffff"
        android:layout_toEndOf="@+id/Totalinch"
        android:layout_marginLeft="49dp"
        android:layout_below="@+id/Totalmiles"
        android:layout_toRightOf="@+id/Totalinch"
        android:layout_marginTop="37dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/Yinch"
        android:textColor="#ffffffff"
        android:layout_marginTop="94dp"
        android:layout_below="@+id/Xinch"
        android:layout_alignLeft="@+id/Xinch"
        android:layout_alignStart="@+id/Xinch" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/Yfeet"
        android:textColor="#ffffffff"
        android:layout_marginBottom="30dp"
        android:layout_above="@+id/Ymiles"
        android:layout_alignLeft="@+id/Ymiles"
        android:layout_alignStart="@+id/Ymiles" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/Totalinch"
        android:textColor="#ffffffff"
        android:layout_alignTop="@+id/Totalmiles"
        android:layout_alignLeft="@+id/Xinch"
        android:layout_alignStart="@+id/Xinch" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/Totalfeet"
        android:textColor="#ffffffff"
        android:layout_marginTop="97dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/Totalmiles"
        android:textColor="#ffffffff"
        android:layout_marginTop="34dp"
        android:layout_below="@+id/Totalfeet"
        android:layout_alignLeft="@+id/Totalfeet"
        android:layout_alignStart="@+id/Totalfeet" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/Ymiles"
        android:layout_alignTop="@+id/Yinch"
        android:layout_alignLeft="@+id/Xfeet"
        android:layout_alignStart="@+id/Xfeet"
        android:textColor="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/XMiles"
        android:layout_alignTop="@+id/Xinch"
        android:layout_alignRight="@+id/Yfeet"
        android:layout_alignEnd="@+id/Yfeet"
        android:textColor="#ffffffff" />

    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignTop="@+id/Totalfeet"
        android:layout_alignRight="@+id/Totalinch"
        android:layout_alignEnd="@+id/Totalinch"
        android:id="@+id/gridLayout"></GridLayout>

    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"></GridLayout>

</RelativeLayout>
